<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.xiaoy.base.entites.RolePopedom" table="ROLE_POPEDOM">
        <id name="roleCode" type="java.lang.String">
            <column name="ROLE_CODE" not-null="true"/>
        </id>
        <property name="popedomCode" type="java.lang.String" length="800">
            <column name="POPEDOM_CODE" />
        </property>
        <property name="remark" type="java.lang.String">
            <column name="REMARK" />
        </property>
    </class>
</hibernate-mapping>
